{
  "name": "@omc-space/api-client",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@8.14.0",
  "description": "_description_",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/omc-space",
  "homepage": "https://github.com/omc-space/satori-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omc-space/satori-core.git"
  },
  "bugs": "https://github.com/omc-space/satori-core/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild && node mod-dts.mjs",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@omc-space/ni": "^0.21.12",
    "@omc-space/utils": "^0.7.7",
    "@types/node": "^20.10.7",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.14.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "dependencies": {
    "vite-tsconfig-paths": "^4.2.3"
  }
}
